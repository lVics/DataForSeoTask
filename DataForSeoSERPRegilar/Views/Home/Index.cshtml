@{
    ViewData["Title"] = "Home Page";
}

@using DataForSeo.SERPRegular.ViewModels;

@model SERPRegularViewModel

  <div class="text-center">
    <h1 class="display-4">Welcome</h1>

    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">First</th>
          <th scope="col">Last</th>
          <th scope="col">Handle</th>
        </tr>
      </thead>
      <tbody>
        @foreach (var regular in Model.SERPRegulars)
        {
          <tr>
            <th scope="row">@regular.Keyword</th>
            <td>@regular.SearchEngine</td>
            <td>Otto</td>
            <td>@mdo</td>
          </tr>
        }
      </tbody>
    </table>
  </div>
